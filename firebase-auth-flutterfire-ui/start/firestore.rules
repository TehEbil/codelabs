rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if
          request.time < timestamp.date(2022, 8, 23);
    }
    match /chats/{chatId} {
      allow read, write: if
          request.time < timestamp.date(2022, 8, 23);
    }
  }
}
